{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "#{template.vm.name}#"
        },
        "location": {
            "value": "#{template.vm.location}#"
        },
        "shutdownTime": {
            "value": "#{template.vm.shutdownTime}#"
        },
        "vmSize": {
            "value": "#{template.vm.size}#"
        },
        "vmSku": {
            "value": "#{template.vm.sku}#"
        },
        "diskType": {
            "value": "#{template.vm.diskType}#"
        },
        "diskSize": {
            "value": #{template.vm.diskSize}#
        },
        "adminUsername": {
            "value": "#{template.vm.adminUsername}#"
        },
        "adminPassword": {
            "value": "#{template.vm.adminPassword}#"
        },
        "vnetRg": {
            "value": "#{template.vnet.rg}#"
        },
        "vnetName": {
            "value": "#{template.vnet.name}#"
        },
        "vnetSubnetName": {
            "value": "#{template.vnet.subnetName}#"
        }
    }
}